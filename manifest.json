{
    "name": "UserAlgo",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Let users customize their own content recommendation algorithm on YouTube",

    "permissions": ["storage", "scripting", "webNavigation", "activeTab", "tabs"],
    "host_permissions": [],

    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/results*"],
        "run_at": "document_start",
        "js": ["yt_modifier.js"],
        "all_frames": true
      }
    ]
}
  