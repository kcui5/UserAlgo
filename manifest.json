{
    "name": "UserAlgo",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Let users customize their own content recommendation algorithm on YouTube",
    "permissions": ["storage", "scripting", "activeTab", "declarativeNetRequestWithHostAccess", "https://www.youtube.com/*"],
    "host_permissions": ["https://www.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "YouTube Blocker",
      "default_popup": "popup.html"
    }
  }
  